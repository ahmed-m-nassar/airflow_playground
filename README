# Install required packages
pip install duckdb pandas
pip install apache-airflow[cncf.kubernetes]

export AIRFLOW__SCHEDULER__DAG_DIR_LIST_INTERVAL=1
export AIRFLOW__SCHEDULER__MIN_FILE_PROCESS_INTERVAL=1
export AIRFLOW__CORE__DAGS_FOLDER=/workspaces/airflow_playground/dags
airflow standalone



export AIRFLOW__SMTP__SMTP_HOST=sandbox.smtp.mailtrap.io
export AIRFLOW__SMTP__SMTP_PORT=2525
export AIRFLOW__SMTP__SMTP_USER=
export AIRFLOW__SMTP__SMTP_PASSWORD=
export AIRFLOW__SMTP__SMTP_MAIL_FROM=Test@example.com
export AIRFLOW__SMTP__SMTP_STARTTLS=True
export AIRFLOW__SMTP__SMTP_SSL=False











